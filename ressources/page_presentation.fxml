<?import javafx.geometry.Insets?>
<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="605.0" prefWidth="1021.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="carnetdevoyage.vues.ControlleurPagePresentation">
  <children>
      <fx:include source="menu.fxml" />
      <BorderPane prefHeight="582.0" prefWidth="1021.0">
         <left>
            <VBox prefHeight="499.0" prefWidth="618.0" BorderPane.alignment="CENTER">
               <children>
                  <Label fx:id="nomauteur" alignment="CENTER" prefHeight="17.0" prefWidth="374.0" text="Nom de l'auteur" wrapText="true">
                     <font>
                        <Font name="Times New Roman Bold" size="19.0" />
                     </font>
                  </Label>
                  <Label fx:id="infosauteur" prefHeight="106.0" prefWidth="593.0" text="Infos auteur : âge, centre d'intérêt..." wrapText="true">
                     <font>
                        <Font size="13.0" />
                     </font>
                  </Label>
                  <ImageView fx:id="imageauteur" fitHeight="235.0" fitWidth="234.0" focusTraversable="true" nodeOrientation="INHERIT" pickOnBounds="true" preserveRatio="true" translateX="80.0">
                     <image>
                        <Image url="@/images/default/auteur.jpg" />
                     </image>
                     <cursor>
                        <Cursor fx:constant="DEFAULT" />
                     </cursor>
                     <viewport>
                        <Rectangle2D />
                     </viewport>
                     <VBox.margin>
                        <Insets bottom="20.0" left="10.0" right="10.0" top="20.0" />
                     </VBox.margin>
                  </ImageView>
                  <Label fx:id="infosoption" prefHeight="71.0" prefWidth="449.0" text="Infos optionnelles sur ta vie, ce que tu aimes bien faire, sur ton voyage, sur ton c hat, sur tes amis, sur ta famille, sur ton poisson rouge probablement encore vivant,..." wrapText="true" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="5.0" />
               </padding>
            </VBox>
         </left>
         <top>
            <VBox prefHeight="44.0" prefWidth="632.0" BorderPane.alignment="CENTER">
               <children>
                  <Label fx:id="titreCarnet" alignment="CENTER" contentDisplay="CENTER" prefHeight="31.0" prefWidth="1073.0" text="TITRE" textAlignment="CENTER" wrapText="true">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Label>
                  <Label fx:id="date" alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="1088.0" text="date debut - date fin" wrapText="true">
                     <font>
                        <Font size="13.0" />
                     </font>
                  </Label>
               </children>
            </VBox>
         </top>
         <right>
            <VBox prefHeight="499.0" prefWidth="365.0" BorderPane.alignment="CENTER">
               <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="398.0" text="Participant(s) :" wrapText="true">
                     <font>
                        <Font name="Times New Roman Bold" size="19.0" />
                     </font>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </Label>
                  <ListView fx:id="listviewParticipant" editable="true" fixedCellSize="10.0" prefHeight="426.0" prefWidth="345.0">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin>
                  </ListView>
               </children>
               <padding>
                  <Insets top="5.0" />
               </padding>
            </VBox>
         </right>
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
      </BorderPane>
      <fx:include source="navigation.fxml" />
  </children>
</VBox>
