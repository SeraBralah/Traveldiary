<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="605.0" prefWidth="1021.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="carnetdevoyage.vues.ControlleurPageDestination">
  <children>
      <BorderPane>
         <left>
            <VBox prefHeight="491.0" prefWidth="454.0" BorderPane.alignment="CENTER">
               <children>
                  <Label fx:id="titre" alignment="CENTER" prefHeight="17.0" prefWidth="374.0" text="Titre du voyage" wrapText="true">
                     <font>
                        <Font name="Times New Roman Bold" size="19.0" />
                     </font>
                  </Label>
                  <Label fx:id="description" prefHeight="446.0" prefWidth="433.0" text="Quelques petites informations sur notre super voyage, il était vraiment génial ce voyage. Incroyable pour ainsi dire." wrapText="true">
                     <font>
                        <Font size="13.0" />
                     </font>
                     <opaqueInsets>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </opaqueInsets>
                  </Label>
                  <Label fx:id="localisation" contentDisplay="CENTER" prefHeight="20.0" prefWidth="338.0" text="Localisation" textAlignment="CENTER" wrapText="true">
                     <font>
                        <Font size="19.0" />
                     </font></Label>
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="5.0" />
               </padding>
            </VBox>
         </left>
         <right>
            <VBox prefHeight="379.0" prefWidth="504.0" BorderPane.alignment="CENTER">
               <padding>
                  <Insets top="5.0" />
               </padding>
               <children>
                  <HBox prefHeight="286.0" prefWidth="504.0">
                     <children>
                        <ImageView fx:id="imageLongue" fitHeight="279.0" fitWidth="176.0" pickOnBounds="true">
                           <HBox.margin>
                              <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                           </HBox.margin>
                        </ImageView>
                        <ImageView fx:id="imageCarre" fitHeight="282.0" fitWidth="290.0" nodeOrientation="INHERIT" pickOnBounds="true">
                           <HBox.margin>
                              <Insets bottom="2.0" left="30.0" right="2.0" top="2.0" />
                           </HBox.margin>
                        </ImageView>
                     </children>
                  </HBox>
                  <HBox prefHeight="283.0" prefWidth="504.0">
                     <children>
                        <ImageView fx:id="imageLongue2" fitHeight="277.0" fitWidth="497.0" pickOnBounds="true" preserveRatio="true">
                           <HBox.margin>
                              <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                           </HBox.margin>
                        </ImageView>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </right>
         <top>
            <fx:include source="menu.fxml" />
         </top>
      </BorderPane>
      <fx:include source="navigation.fxml" />
  </children>
</VBox>
