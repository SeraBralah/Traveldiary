                        <ImageView fitHeight="131.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true">
                           <HBox.margin>
                              <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                           </HBox.margin>
                        </ImageView>
                        <ImageView fitHeight="131.0" fitWidth="150.0" nodeOrientation="INHERIT" pickOnBounds="true" preserveRatio="true">
                           <HBox.margin>
                              <Insets bottom="2.0" left="30.0" right="2.0" top="2.0" />
                           </HBox.margin>
                        </ImageView>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </right>
         <bottom>
            <HBox prefHeight="25.0" prefWidth="632.0" BorderPane.alignment="CENTER">
               <children>
                  <Button alignment="CENTER_RIGHT" contentDisplay="RIGHT" mnemonicParsing="false" prefHeight="25.0" prefWidth="112.0" text="&lt;" />
                  <Label alignment="CENTER" prefHeight="25.0" prefWidth="657.0" text="0/...">
